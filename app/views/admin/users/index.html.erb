<h1>Admin Page </h1>

<%= link_to "Back to all movies", movies_path %> <br>
<%= link_to "Create a new User" , new_admin_user_path %>
<hr>
<% @users.each do |user| %>
  <p> Username: <%= user.full_name %> <%= link_to "Edit", edit_admin_user_path(user) %>
  <%= link_to "Delete", admin_user_path(user), method: :delete, data: { confirm: 'You sure?' } %></p>
  <%= link_to "See more details", admin_user_path(user) %>
  <hr>


<% end %>

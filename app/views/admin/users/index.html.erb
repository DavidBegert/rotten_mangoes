<h1>Admin Page </h1>
<%= link_to "Create a new User" , new_admin_user_path %>
<% @users.each do |user| %>
  <p> Username: <%= user.full_name %> <%= link_to "Edit User", edit_admin_user_path(user) %>
  <%= link_to "Delete User", admin_user_path(user), method: :delete, data: { confirm: 'You sure?' } %></p>
  <%= link_to "See more details", admin_user_path(user) %>


<% end %>
